{
  "name": "@packages/pinia-plugin-persist",
  "version": "1.0.0",
  "description": "pinia持久化",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "module": "dist/index.esm-bundler.js",
  "types": "dist/index.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.esm-bundler.js",
      "import": "./dist/index.esm-bundler.js"
    },
    "./*": "./dist/*"
  },
  "buildOptions": {
    "formats": [
      "esm-bundler"
    ],
    "needType": true
  },
  "license": "MIT",
  "peerDependencies": {
    "localforage": ">=1.0.0",
    "pinia": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "pinia": {
      "optional": true
    }
  },
  "devDependencies": {
    "@packages/eslint": "workspace:^",
    "@packages/prettier": "workspace:^"
  },
  "dependencies": {
    "deep-pick-omit": "^1.2.1",
    "localforage": "^1.10.0",
    "pinia": "^3.0.4"
  }
}