{
  "name": "@packages/pinia-plugin-persist",
  "version": "1.0.0",
  "description": "pinia持久化",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "node ./esbuild.config.js",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./dist/*"
  },
  "license": "MIT",
  "peerDependencies": {
    "localforage": ">=1.0.0",
    "pinia": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "pinia": {
      "optional": true
    }
  },
  "devDependencies": {
    "@packages/eslint": "workspace:^",
    "@packages/prettier": "workspace:^",
    "esbuild": "^0.25.8",
    "esbuild-plugin-d.ts": "^1.3.1",
    "globals": "^17.0.0"
  },
  "dependencies": {
    "deep-pick-omit": "^1.2.1",
    "localforage": "^1.10.0",
    "pinia": "^3.0.4"
  }
}